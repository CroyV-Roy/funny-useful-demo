{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "no-undef":"off"
    }
}
